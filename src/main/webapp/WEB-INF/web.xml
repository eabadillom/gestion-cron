<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<listener>
		<listener-class>org.quartz.ee.servlet.QuartzInitializerListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>ReporteInventarioServlet</servlet-name>
		<display-name>ReporteInventarioServlet</display-name>
		<description></description>
		<servlet-class>com.ferbo.cron.servlet.ReporteInventarioServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReporteEntradaServlet</servlet-name>
		<display-name>ReporteEntradaServlet</display-name>
		<description></description>
		<servlet-class>com.ferbo.cron.servlet.ReporteEntradaServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReporteSalidaServlet</servlet-name>
		<display-name>ReporteSalidaServlet</display-name>
		<description></description>
		<servlet-class>com.ferbo.cron.servlet.ReporteSalidaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ReporteInventarioServlet</servlet-name>
		<url-pattern>/reporte-inventario</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReporteEntradaServlet</servlet-name>
		<url-pattern>/reporte-entrada</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReporteSalidaServlet</servlet-name>
		<url-pattern>/reporte-salida</url-pattern>
	</servlet-mapping>
</web-app>
